<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.uzair.kptehsilmunicipaladministrationservices">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_foreground"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_foreground"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Certificates.CertificatesMain"></activity>
        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <service
            android:name="FirebaseMessagingService.FirebaseMessagingService"
            android:exported="false">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>

        <activity
            android:name=".Certificates.DeathCertificate"
            android:parentActivityName=".Main.MainActivity"
            android:screenOrientation="locked" />
        <activity
            android:name=".Certificates.BirthCertificate"
            android:parentActivityName=".Main.MainActivity"
            android:screenOrientation="locked" />
        <activity
            android:name=".UserProfile.Profile"
            android:parentActivityName=".Main.MainActivity"
            android:screenOrientation="locked" />
        <activity
            android:name=".BuildingNoc.BuildingNocActivity"
            android:parentActivityName=".Main.MainActivity"
            android:screenOrientation="locked" />
        <activity
            android:name=".TaxesDetails.Taxes"
            android:parentActivityName=".Main.MainActivity"
            android:screenOrientation="locked" />
        <activity
            android:name=".WaterBills.WaterBills"
            android:parentActivityName=".Main.MainActivity"
            android:screenOrientation="locked" />
        <activity
            android:name=".UserComplaint.ComplaintsMain.UserFeedBack"
            android:parentActivityName=".UserComplaint.ComplaintsMain.Complaints"
            android:screenOrientation="locked" />
        <activity
            android:name=".UserComplaint.ComplaintsMain.AddComplaints"
            android:parentActivityName=".UserComplaint.ComplaintsMain.Complaints"
            android:screenOrientation="locked" />
        <activity
            android:name=".UserComplaint.ComplaintsMain.Complaints"
            android:parentActivityName=".Main.MainActivity"
            android:screenOrientation="locked" />
        <activity
            android:name=".LoginAndSignUp.SignUp"
            android:screenOrientation="locked" />
        <activity
            android:name=".LoginAndSignUp.Login"
            android:screenOrientation="locked" />
        <activity
            android:name=".Main.MainActivity"
            android:screenOrientation="locked"></activity>
    </application>

</manifest>